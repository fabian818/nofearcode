	<section id="contact">
		<div class="container">
			<div class="row">
				<div class="col-lg-12 text-center">
					<h2 class="section-heading">Contáctanos</h2>
					<h3 class="section-subheading text-muted">Antes que nosotros contactemos con usted.</h3>
				</div>
			</div>
			<div class="row">
				<div class="col-lg-12">
					<%= form_tag(mail_path, method: 'post', id: 'contactForm') do %>
					<div class="row">
						<div class="col-md-6">
							<div class="form-group control-group">
								<%= text_field_tag :name, '', required: true, placeholder: 'Su nombre o de su empresa *', 'required data-validation-required-message' => 'Please enter your name.', class: 'form-control' %>
								<p class="help-block text-danger"></p>
							</div>
							<div class="form-group control-group">
								<%= email_field_tag :email, '', placeholder: 'Su email *', 'required data-validation-required-message' => 'Please enter your email address.', class: 'form-control' %>
								<p class="help-block text-danger"></p>
							</div>
							<div class="form-group control-group">
								<%= text_field_tag :phone, '', required: true, placeholder: 'Su teléfono *', 'required data-validation-required-message' => 'Please enter your phone number.', class: 'form-control', pattern: "[#|0-9]+$"  %>
								<p class="help-block text-danger"></p>
							</div>
						</div>
						<div class="col-md-6">
							<div class="form-group control-group">
								<%= text_area_tag :message, '', placeholder: 'Su mensaje o propuesta *', 'required data-validation-required-message' => 'Please enter a message', class: 'form-control' %>
								<p class="help-block text-danger"></p>
							</div>
						</div>
						<div class="clearfix"></div>
						<div class="col-lg-12 text-center">
							<div id="success"></div>
							<button id="btn-submit" type="submit" class="btn btn-xl">Enviar Propuesta</button>
						</div>
					</div>
					<% end %>
				</div>
			</div>
		</div>
	</section>

